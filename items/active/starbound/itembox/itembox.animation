{
  "animatedParts": {
    "stateTypes": {
      "firing": {
        "default": "off",
        "states": {
          "off": {
            "properties": {
              "particleEmittersOff": ["confetti"]
            }
          },
          "fire": {
            "frames": 2,
            "cycle": 0.1,
            "mode": "transition",
            "transition": "confetti",
            "properties": {
              "immediateSound": "/sfx/objects/colonydeed_partyhorn.ogg",
              "particleEmittersOff": ["confetti"]
            }
          },
          "confetti": {
            "cycle": 0.2,
            "mode": "transition",
            "transition": "off",
            "properties": {
              "particleEmittersOn": ["confetti"]
            }
          }
        }
      }
    },

    "parts": {
      "gun": {
        "properties": {
          "centered": true,
          "image": "<partImage>",
          "offset": [0, 0]
        }
      }
    }
  },

